include ../tools.mk

all:
	# Verbatim allows specify precise name.
	$(CRABLANGC) upstream_native_dep.rs --crate-type=staticlib -o $(TMPDIR)/upstream_some_strange_name.ext
	$(CRABLANGC) crablang_dep.rs -l static:+verbatim=upstream_some_strange_name.ext --crate-type rlib

	# With verbatim any other name cannot be used (upstream).
	$(CRABLANGC) upstream_native_dep.rs --crate-type=staticlib -o $(TMPDIR)/libupstream_native_dep.a
	$(CRABLANGC) upstream_native_dep.rs --crate-type=staticlib -o $(TMPDIR)/upstream_native_dep.a
	$(CRABLANGC) upstream_native_dep.rs --crate-type=staticlib -o $(TMPDIR)/upstream_native_dep.lib
	$(CRABLANGC) crablang_dep.rs -l static:+verbatim=upstream_native_dep --crate-type rlib 2>&1 | $(CGREP) "upstream_native_dep"
